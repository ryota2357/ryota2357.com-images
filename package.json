    "@types/walk": "2.3.1",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "volta": {
    "node": "18.17.1"
  }
